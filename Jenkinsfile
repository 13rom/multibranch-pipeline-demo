pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                sh 'echo "Building..."'
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo "Deploying..."'
            }
        }
        stage('Healthcheck') {
            steps {
                sh 'echo "Healthchecking..."'
            }
        }
    }
}
